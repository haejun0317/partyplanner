<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="kr.co.partyplanner.mapper.McMapper">
	<select id="listAll" resultType="Mc">
		SELECT NAME, 
		       price,
		       profile 
		FROM   mc
		ORDER  BY price
	</select>
</mapper>